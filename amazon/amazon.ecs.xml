<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Nagesh Susarla</author>
    <documentationURL>http://docs.amazonwebservices.com/AWSECommerceService/2009-01-06/GSG/<documentationURL>
  </meta>
  <bindings>
    <select itemPath="feed.entry" produces="XML">
      <urls>
        <url>http://ecs.amazonaws.com/onca/xml?Service=AWSECommerceService&AWSAccessKeyId={api_key}&amp;Operation=ItemSearch&amp;SearchIndex=Books&Version=2008-08-19</url>
      </urls>
      <paging model="page">
        <start default="0" id="page"/>
        <pagesize max="200" id="count"/>
        <total default="20"/>
      </paging>
      <inputs>
        <key id="Title" type="xs:string" paramType="query" required="true"/>
        <key id="api_key" type="xs:string" paramType="path" required="true"/>
       </inputs>
    </select>
  </bindings>
</table>
